# .env.oauth - ReactVector OAuth2 Auth Configuration

# 🌐 Base API endpoint used for login, logout, and user info
VITE_AUTH_API=http://localhost:7090/api

# ✅ Enable if your backend enriches user data after login
VITE_ENABLE_HYBRID_AUTH=true

# 🧪 Dev-only: enables fallback to test adapter if no auth adapter is registered
VITE_REACTVECTOR_DEV_TOKEN=ENABLED

